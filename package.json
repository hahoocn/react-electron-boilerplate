{
  "name": "react-electron-boilerplate",
  "productName": "MyApp",
  "version": "0.3.2",
  "description": "Boilerplate app for Electron with React",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=production electron ./build/main",
    "start:hot": "cross-env NODE_ENV=development babel-node webpack/webpack.devServer.js",
    "start:dev": "cross-env NODE_ENV=development electron -r babel-register -r babel-polyfill ./src/electron",
    "dev": "concurrently --kill-others \"npm run start:hot\" \"npm run start:dev\"",
    "build:page": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --progress",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.electron.js --progress",
    "build": "npm run build:page && npm run build:main",
    "package": "cross-env NODE_ENV=production node package.js ",
    "clean": "rimraf build",
    "clean:release": "rimraf release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hahoocn/react-electron-boilerplate.git"
  },
  "keywords": [
    "electron",
    "react",
    "boilerplate",
    "redux"
  ],
  "author": "Ray Guo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hahoocn/react-electron-boilerplate/issues"
  },
  "homepage": "https://github.com/hahoocn/react-electron-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "clean-webpack-plugin": "^0.1.13",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "devtron": "^1.4.0",
    "electron": "^1.4.4",
    "electron-packager": "^8.1.0",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.24.0",
    "image-webpack-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "postcss": "^5.2.5",
    "postcss-loader": "^1.1.0",
    "postcss-mixins": "^5.4.0",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "react-hot-loader": "^1.3.0",
    "remote-redux-devtools": "^0.5.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "concurrently": "^3.1.0",
    "hahoorequest": "^0.6.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-act-reducer": "^1.5.2",
    "redux-thunk": "^2.1.0"
  }
}
