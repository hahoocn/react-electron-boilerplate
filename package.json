{
  "name": "react-electron-boilerplate",
  "version": "0.2.0",
  "description": "Boilerplate app for Electron with React",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=production electron ./build/main",
    "start:hot": "cross-env NODE_ENV=development babel-node webpack/webpack.devServer.js",
    "start:dev": "cross-env NODE_ENV=development electron -r babel-register -r babel-polyfill ./src/electron",
    "dev": "npm run start:hot & npm run start:dev",
    "build:page": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js --progress",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.electron.js --progress",
    "build": "npm run build:page && npm run build:main",
    "clean": "rimraf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hahoocn/react-electron-boilerplate.git"
  },
  "keywords": [
    "electron",
    "react",
    "boilerplate",
    "redux"
  ],
  "author": "Ray Guo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hahoocn/react-electron-boilerplate/issues"
  },
  "homepage": "https://github.com/hahoocn/react-electron-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "clean-webpack-plugin": "^0.1.9",
    "cross-env": "^1.0.8",
    "css-loader": "^0.23.1",
    "devtron": "^1.2.1",
    "electron-packager": "^7.3.0",
    "electron-prebuilt": "^1.2.6",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.4.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.9.1",
    "postcss-mixins": "^5.0.0",
    "postcss-nested": "^1.0.0",
    "postcss-simple-vars": "^3.0.0",
    "react-hot-loader": "^1.3.0",
    "remote-redux-devtools": "^0.3.3",
    "rimraf": "^2.5.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "hahoorequest": "^0.5.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-act-reducer": "^1.4.2",
    "redux-thunk": "^2.1.0"
  }
}
